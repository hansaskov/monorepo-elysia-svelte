		"@types/eslint": "^9.6.1",
		"backend": "workspace:*",
		"elysia": "^1.1.25",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.12.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.8",
		"svelte": "^5.2.6",
		"svelte-check": "^4.0.9",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.15.0",
		"vite": "^5.4.11"
	},
	"dependencies": {
		"@elysiajs/eden": "^1.1.3",
